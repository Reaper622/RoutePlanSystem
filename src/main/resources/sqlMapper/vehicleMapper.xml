<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.may.routeplansystem.dao.VehicleDao">    <resultMap id="vehicleMap" type="com.may.routeplansystem.pojo.VehicleMessage">        <result property="vehicleId" column="vehicle_id" jdbcType="INTEGER"/>        <result property="questionId" column="question_id" jdbcType="INTEGER"/>        <result property="type" column="type" jdbcType="VARCHAR"/>        <result property="capacity" column="capacity" jdbcType="FLOAT"/>        <result property="oil" column="oil" jdbcType="FLOAT"/>        <result property="date" column="date" jdbcType="TIMESTAMP"/>        <result property="price" column="price" jdbcType="FLOAT"/>        <result property="delFlag" column="del_flag" jdbcType="TINYINT"/>        <result property="ownerId" column="owner_id" jdbcType="INTEGER"/>    </resultMap>    <sql id="Base_Vehicle_List">        vehicle_id,question_id,type,capacity,oil,date,price,del_flag,owner_id    </sql>    <insert id="insertVehicle" parameterType="com.may.routeplansystem.pojo.VehicleMessage">        INSERT        INTO vehicleMessage(question_id,type,capacity,oil,date,price,del_flag,owner_id)        VALUE (#{questionId},#{type},#{capacity},#{oil},#{date},#{price},#{delFlag},#{ownerId})    </insert>    <select id="searchVehicleByOwnId" parameterType="java.lang.Integer" resultMap="vehicleMap">        SELECT        <include refid="Base_Vehicle_List"/>        FROM vehicleMessage        WHERE owner_id = #{ownerId}    </select>    <delete id="deleteVehicle">        DELETE        FROM vehicleMessage        WHERE vehicle_id IN        <foreach item="deleteList" collection="list"                 open="(" separator="," close=")">            #{deleteList}        </foreach>    </delete></mapper>